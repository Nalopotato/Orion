function GetProperties()
{
	var props = Orion.FindObject('0x423892B0').Properties();
	var result = '';
	
	Orion.Print('PROPS: ' + props + ' ...END PROPS');
	
	for (var i = props.length - 3; i < props.length; i++)
	{
		if (parseInt(props[i]))
			result += props[i];
		Orion.Print(result);
	}
	
	return result;
}














