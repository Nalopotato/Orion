//For other mobiles, this is proximity based.  Make sure you are close enough to the other player!
function PrintPlayerCoords()
{
    Orion.Print('X: ' + Player.X() + ' ' + 'Y: ' + Player.Y() + ' ' + 'Z: ' + Player.Z()); 
}

function WritePlayerCoords()
{
    Orion.Print('X: ' + Player.X() + ' ' + 'Y: ' + Player.Y() + ' ' + 'Z: ' + Player.Z()); 
}