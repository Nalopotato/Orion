function PathTo(startX, x, y)
{
	//Check player is where they should be
	if (startX != 'any' && startX > 0 && Player.X() != startX)
		return false;

	Orion.WalkTo(x, y, any, 0, 255, 1, 1);
	
	if (Player.X() != x && Player.Y() != y)
		Orion.WalkTo(x, y, any, 0, 255, 1, 1, 10000);

	return true;
}