function GetProperties()
{
	Orion.WaitForAddObject('Target');;
	var props = Orion.FindObject('Target').Properties();
	var result = '';
	
	Orion.Print('_____ Properties _____ \n' + props + '\n ________________');
	
	for (var i = props.length - 3; i < props.length; i++)
	{
		if (parseInt(props[i]))
			result += props[i];
		Orion.Print(result);
	}
	
	return result;
}














